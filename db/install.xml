<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/infosysselfesteem/db" VERSION="20101203" COMMENT="XMLDB file for Moodle mod/infosysselfesteem"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="infosysselfesteem" COMMENT="Default comment for infosysselfesteem, please edit me">
      <FIELDS>
        <FIELD NAME="id"            TYPE="int"  LENGTH="10"     NOTNULL="true"   UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="course"        TYPE="int"  LENGTH="10"     NOTNULL="true"   UNSIGNED="true" SEQUENCE="false"              COMMENT="Course infosysselfesteem activity belongs to"/>
        <FIELD NAME="name"          TYPE="char" LENGTH="255"    NOTNULL="true"                   SEQUENCE="false"              COMMENT="name field for moodle instances"/>
        <FIELD NAME="intro"         TYPE="text"                 NOTNULL="true"                   SEQUENCE="false"              COMMENT="General introduction of the infosysselfesteem activity"/>
        <FIELD NAME="introformat"   TYPE="int"  LENGTH="4"      NOTNULL="true"   UNSIGNED="true" SEQUENCE="false" DEFAULT="0"  COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)"/>
        <FIELD NAME="timecreated"   TYPE="int"  LENGTH="10"     NOTNULL="true"   UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified"  TYPE="int"  LENGTH="10"     NOTNULL="true"   UNSIGNED="true" SEQUENCE="false" DEFAULT="0" />
        <FIELD NAME="grade"         TYPE="int"  LENGTH="10"     NOTNULL="true"                   SEQUENCE="false" DEFAULT="100" COMMENT="The maximum grade. Can be negative to indicate the use of a scale."/>

        <FIELD NAME="year"    TYPE="int"  LENGTH="4" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="year"/>
        <FIELD NAME="subject" TYPE="char" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="A" COMMENT="subject"/>
        <FIELD NAME="times"   TYPE="int"  LENGTH="2" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="times"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>

    <TABLE NAME="infosysselfesteem_rubric" COMMENT="rubric sheet">
      <FIELDS>
        <FIELD NAME="id"            TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" />
        <FIELD NAME="user_id"       TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="infosysselfesteem_id"    TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_1"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_2"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_3"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_4"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_5"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_6"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_7"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_8"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_9"      TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_10"     TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="rubric_11"     TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="timecreated"   TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified"  TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>

      <KEYS>
        <KEY NAME="primary"       TYPE="primary" FIELDS="id"/>
        <KEY NAME="user_id"       TYPE="foreign" FIELDS="user_id"       REFTABLE="user"       REFFIELDS="id"/>
        <KEY NAME="infosysselfesteem_id"    TYPE="foreign" FIELDS="infosysselfesteem_id"    REFTABLE="infosysselfesteem"    REFFIELDS="id"/>
      </KEYS>

    </TABLE>
  </TABLES>
</XMLDB>
